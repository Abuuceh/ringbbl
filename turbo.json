{
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "lib/**",
        "build/**",
        "homebridge-ui/public/**",
        "homebridge-ui/server.js"
      ]
    },
    "lint": {
      "outputs": []
